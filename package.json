{
  "name": "equinusocio-dev",
  "version": "0.0.0",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn ts:check && yarn test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": "eslint",
    "*.css": "stylelint"
  },
  "browserslist": [
    "last 1 Chrome versions",
    "last 1 Firefox versions",
    "last 1 Safari versions",
    "last 1 Edge versions"
  ],
  "scripts": {
    "predev": "yarn generate:icons",
    "dev": "next dev",
    "prebuild": "yarn generate:icons",
    "build": "next build",
    "export": "next export -o dist",
    "start": "next start",
    "generate:icons": "yarn rimraf src/components/Icons/**/* && svgr --config-file svgr.config.js -d src/components/Icons/ src/icons",
    "update:deps": "yarn upgrade-interactive --latest && yarn upgrade",
    "ts:check": "tsc --noEmit"
  },
  "dependencies": {
    "@svgr/cli": "5.5.0",
    "clsx": "1.1.1",
    "contentful": "8.4.2",
    "cssnano-preset-default": "5.1.3",
    "feed": "4.2.2",
    "framer-motion": "4.1.17",
    "gsap": "3.7.0",
    "marked": "2.1.1",
    "next": "11.0.0",
    "next-compose-plugins": "2.2.1",
    "nprogress": "0.2.0",
    "postcss-dark-theme-class": "0.5.2",
    "prism-react-renderer": "1.2.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-markdown": "6.0.2",
    "react-use": "17.2.4",
    "rehype-raw": "5.1.0",
    "rimraf": "3.0.2",
    "sanitize-html": "2.4.0",
    "what-input": "5.2.10"
  },
  "devDependencies": {
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@lasalefamine/eslint-config": "1.0.0",
    "@next/bundle-analyzer": "11.0.0",
    "@types/marked": "2.0.3",
    "@types/node": "15.12.4",
    "@types/nprogress": "0.2.0",
    "@types/react": "17.0.11",
    "@types/react-syntax-highlighter": "13.5.1",
    "@types/sanitize-html": "2.3.1",
    "@typescript-eslint/eslint-plugin": "4.27.0",
    "@typescript-eslint/parser": "4.27.0",
    "babel-plugin-module-resolver": "4.1.0",
    "cssnano": "5.0.6",
    "cssnano-preset-advanced": "5.1.3",
    "eslint": "7.28.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "6.0.0",
    "lint-staged": "10.5.4",
    "postcss": "8.3.4",
    "postcss-cli": "8.3.1",
    "postcss-easy-import": "3.0.0",
    "postcss-help-media-queries": "1.0.0",
    "postcss-inset": "1.0.0",
    "postcss-mixins": "6.2.3",
    "postcss-preset-env": "6.7.0",
    "stylelint": "13.13.1",
    "stylelint-config-equinusocio": "1.0.5",
    "typescript": "4.3.4",
    "typescript-plugin-css-modules": "3.4.0"
  }
}
